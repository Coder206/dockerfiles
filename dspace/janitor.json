{
    "name": "DSpace",
    "description": "DSpace is a turnkey institutional repository application.",
    "icon": "https://janitor.technology/img/dspace.svg",
    "docker": {
      "image": "janitortechnology/dspace"
    },
    "3000": {
      "label": "VNC",
      "proxy": "https",
      "preview": true
    },
    "8089": {
      "label": "Cloud9",
      "proxy": "https"
    },
    "8090": {
      "label": "Theia",
      "proxy": "https"
    }
  },
  "scripts": {
      "Build": {
        "cmd": [
          "yarn run global",
          "yarn install"
        ]
      },
      "Start": {
        "cmd": "yarn start",
        "openPort": "3000"
      },
      "Watch": {
        "cmd": "yarn run watch",
        "openPort": "3000"
      },
      "Clean": "yarn run clean",
      "Run test": "yarn run test",
      "Run e2e": "yarn run e2e",
      "Update source code": "git pull --rebase origin master",
      "Send to code review": "hub pull-request"
  }
}
